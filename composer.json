{
    "name": "kalyabin/yii2-git-view",
    "description": "Yii2 extension to view and to control GIT repositories",
    "type": "extension",
    "license": "BSD-3-Clause",
    "version": "0.1.5-dev",
    "keywords": [
        "yii2", "extension", "git", "repository"
    ],
    "authors": [
        {
            "name": "Max Kalyabin",
            "email": "maksim@kalyabin.ru"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.5.0",
        "yiisoft/yii2": "2.*",
        "kalyabin/yii2-vcs-common": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "GitView\\": "src/"
        }
    }
}
